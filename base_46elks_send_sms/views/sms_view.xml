<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sms_form_view" model="ir.ui.view">
            <field name="name">sms.sms.form</field>
            <field name="model">sms.sms</field>
            <field name="inherit_id" ref="sms.sms_tsms_view_form"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="after">
                    <field name="46elks_sms_id"/>
                    <field name="rec_model"/>
                </field>
                <field name="number" position="after">
                    <field name="46elks_sms_status"/>
                    <field name="rec_id"/>
                </field>
            </field>
        </record>
    </data>
</odoo>
