<odoo>
    <data>
        <record id="view_partner_form_education1" model="ir.ui.view">
            <field name="name">res.partner.education.form1</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="partner_view_360.view_jobseeker_form" />
            <field name="arch" type="xml">
                <xpath expr="//page[@name='apply_for_jobs']" position="inside">
                    <page name="education" string="Education">
                        <group>
                            <field name="sun_ids" options="{'no_create_edit': True}" widget="many2many_tags" />
                            <field name="education_level" />
                            <field name="foreign_education" />
                            <field name="foreign_education_approved" />
                            <!-- <field name="has_drivers_license" /> -->
                            <field name="drivers_license_ids" options="{'no_create_edit': True}" widget="many2many_tags" />
                            <!-- bortkommenterad fÃ¶r att vi inte vet om vi kan fÃ¥ en integration i tid (Fredrik) -->
                            <field name="has_car" groups="base.group_no_one" />
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
