<odoo>
    <data>
        <record id="view_partner_form_education1" model="ir.ui.view">
            <field name="name">res.partner.education.form1</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="partner_view_360.view_jobseeker_form" />
            <field name="arch" type="xml">
                <xpath expr="//page[@name='contacts_addresses']" position="after">
                    <page name="education" string="Education">
                        <field name="education_ids" mode="tree" nolabel="1">
                            <tree editable="top" string="Educations">
                                <field name="sun_id"/>
                                <field name="education_level_id"/>
                                <field name="foreign_education" invisible="1"/>
                                <field name="foreign_education_approved" invisible="1"/>
                            </tree>
                        </field>
                        <group invisible="1">
                            <field name="has_drivers_license" invisible="1" />
                            <field name="drivers_license_ids" options="{'no_create_edit': True}" widget="many2many_tags" invisible="1" />
                            <field name="has_car" groups="base.group_no_one" invisible="1" />
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
